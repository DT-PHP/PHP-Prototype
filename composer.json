{
  "name": "spiral/prototyping",
  "type": "library",
  "description": "Automatic dependency injection",
  "license": "MIT",
  "authors": [
    {
      "name": "Anton Titov / Wolfy-J",
      "email": "wolfy.jd@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "spiral/core": "^1.0",
    "spiral/tokenizer": "^2.1",
    "nikic/php-parser": "^4.1",
    "spiral/debug": "^1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Spiral\\Prototyping\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Spiral\\Prototyping\\Tests\\": "tests/Prototyping/"
    }
  }
}
