{
  "name": "spiral/prototype",
  "type": "library",
  "description": "Automatic dependency injection",
  "license": "MIT",
  "authors": [
    {
      "name": "Anton Titov / Wolfy-J",
      "email": "wolfy.jd@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "nikic/php-parser": "^4.1",
    "doctrine/inflector": "^1.3",
    "spiral/console": "^1.1 || ^2.5.0 || ^3.4.17 || ^4.1.6 || ^5.0",
    "spiral/framework": "^2.0",
    "spiral/annotations": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "^1.1",
    "cycle/orm": "^1.1",
    "spiral/debug": "^1.3",
    "spiral/code-style": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Spiral\\Prototype\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Spiral\\Prototype\\Tests\\": "tests/Prototype/"
    }
  }
}
